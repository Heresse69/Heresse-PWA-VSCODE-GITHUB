{
  "tables": {
    "user_profiles": {
      "description": "Profil utilisateur principal",
      "columns": {
        "id": "uuid PRIMARY KEY",
        "auth_user_id": "uuid REFERENCES auth.users(id)",
        "first_name": "text NOT NULL",
        "birthdate": "date NOT NULL",
        "bio": "text",
        "avg_rating": "decimal",
        "verified_photo_url": "text",
        "last_seen": "timestamptz",
        "city": "text",
        "latitude": "decimal",
        "longitude": "decimal",
        "is_premium": "boolean DEFAULT false",
        "is_incognito": "boolean DEFAULT false",
        "created_at": "timestamptz DEFAULT now()"
      }
    },
    "public_photos": {
      "description": "Photos publiques (cartes de profil)",
      "columns": {
        "id": "uuid PRIMARY KEY",
        "user_id": "uuid REFERENCES user_profiles(id)",
        "image_url": "text NOT NULL",
        "is_main": "boolean DEFAULT false",
        "order_index": "integer",
        "created_at": "timestamptz DEFAULT now()"
      }
    },
    "private_gallery": {
      "description": "M\u00e9dias priv\u00e9s (max 25, 10 vid\u00e9os max)",
      "columns": {
        "id": "uuid PRIMARY KEY",
        "user_id": "uuid REFERENCES user_profiles(id)",
        "media_url": "text NOT NULL",
        "is_video": "boolean DEFAULT false",
        "price": "decimal",
        "created_at": "timestamptz DEFAULT now()"
      }
    },
    "media_ratings": {
      "description": "Notes des m\u00e9dias priv\u00e9s",
      "columns": {
        "id": "uuid PRIMARY KEY",
        "media_id": "uuid REFERENCES private_gallery(id)",
        "rater_id": "uuid REFERENCES user_profiles(id)",
        "rating": "integer CHECK (rating >= 1 AND rating <= 5)",
        "created_at": "timestamptz DEFAULT now()"
      },
      "constraints": [
        "UNIQUE(media_id, rater_id)"
      ]
    },
    "stories": {
      "description": "Stories utilisateurs (max 10, 10s max)",
      "columns": {
        "id": "uuid PRIMARY KEY",
        "user_id": "uuid REFERENCES user_profiles(id)",
        "media_url": "text NOT NULL",
        "is_video": "boolean DEFAULT false",
        "created_at": "timestamptz DEFAULT now()"
      }
    },
    "swipes": {
      "description": "Like / Dislike entre utilisateurs",
      "columns": {
        "id": "uuid PRIMARY KEY",
        "swiper_id": "uuid REFERENCES user_profiles(id)",
        "swiped_id": "uuid REFERENCES user_profiles(id)",
        "is_like": "boolean NOT NULL",
        "created_at": "timestamptz DEFAULT now()"
      },
      "constraints": [
        "UNIQUE(swiper_id, swiped_id)",
        "CHECK(swiper_id != swiped_id)"
      ]
    },
    "matches": {
      "description": "Matchs mutuels",
      "columns": {
        "id": "uuid PRIMARY KEY",
        "user1_id": "uuid REFERENCES user_profiles(id)",
        "user2_id": "uuid REFERENCES user_profiles(id)",
        "is_active": "boolean DEFAULT true",
        "created_at": "timestamptz DEFAULT now()"
      },
      "constraints": [
        "UNIQUE(user1_id, user2_id)",
        "CHECK(user1_id < user2_id)"
      ]
    },
    "messages": {
      "description": "Messages entre utilisateurs match\u00e9s",
      "columns": {
        "id": "uuid PRIMARY KEY",
        "match_id": "uuid REFERENCES matches(id)",
        "sender_id": "uuid REFERENCES user_profiles(id)",
        "text": "text",
        "media_url": "text",
        "created_at": "timestamptz DEFAULT now()"
      }
    },
    "user_preferences": {
      "description": "Pr\u00e9f\u00e9rences de filtre",
      "columns": {
        "user_id": "uuid PRIMARY KEY REFERENCES user_profiles(id)",
        "min_age": "integer DEFAULT 18",
        "max_age": "integer DEFAULT 99",
        "max_distance_km": "integer DEFAULT 50",
        "show_verified_only": "boolean DEFAULT false"
      }
    }
  },
  "storage_buckets": {
    "profile.photos": {
      "public": true,
      "file_size_limit": "10MB",
      "allowed_mime_types": [
        "image/jpeg",
        "image/png",
        "image/webp"
      ]
    },
    "private.gallery": {
      "public": false,
      "file_size_limit": "50MB"
    },
    "kyc.docs": {
      "public": false,
      "file_size_limit": "10MB"
    },
    "stories.media": {
      "public": true,
      "file_size_limit": "10MB"
    },
    "chat.media": {
      "public": false,
      "file_size_limit": "20MB"
    }
  },
  "functions": {
    "calculate_distance": {
      "parameters": [
        "lat1 decimal",
        "lon1 decimal",
        "lat2 decimal",
        "lon2 decimal"
      ],
      "returns": "decimal"
    },
    "check_mutual_match": {
      "parameters": [
        "user1_id uuid",
        "user2_id uuid"
      ],
      "returns": "boolean"
    },
    "get_swipe_recommendations": {
      "parameters": [
        "user_id uuid",
        "limit_count integer"
      ],
      "returns": "setof user_profiles"
    }
  },
  "rls_policies": {
    "user_profiles": {
      "select": "auth.uid() IS NOT NULL",
      "insert": "auth.uid() = auth_user_id",
      "update": "auth.uid() = auth_user_id"
    }
  },
  "indexes": {
    "user_profiles": [
      "auth_user_id",
      "last_seen",
      "(latitude, longitude)"
    ],
    "swipes": [
      "swiper_id",
      "swiped_id",
      "is_like"
    ],
    "public_photos": [
      "user_id",
      "is_main",
      "order_index"
    ]
  }
}